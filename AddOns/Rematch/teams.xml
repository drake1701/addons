					<Size x="26" y="26"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</HighlightTexture>
				<Scripts>
					<OnEnter>
						Rematch.PreferencesOnEnter(self) -- defined in leveling.lua
					</OnEnter>
					<OnLeave>
						RematchTooltip:Hide()
					</OnLeave>
					<OnClick>
						Rematch.EditPreferences(self)
					</OnClick>
				</Scripts>
			</Button>
