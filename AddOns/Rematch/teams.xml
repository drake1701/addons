		</Backdrop>
		<Frames>
			<!-- this is not used for scrolling; it's to use a scrollChild so tabs clip at the bottom -->
			<ScrollFrame parentKey="scrollFrame">
				<Anchors>
					<Anchor point="TOPLEFT" relativePoint="TOPRIGHT" x="0" y="0"/>
					<Anchor point="BOTTOMRIGHT" x="48" y="0"/>
				</Anchors>
				<ScrollChild>
					<Frame parentKey="childFrame">
