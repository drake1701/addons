		<Frames>
			<Button parentArray="pets" inherits="RematchTeamCardPetTemplate">
				<Anchors>
					<Anchor point="TOPLEFT" x="20" y="-52"/>
				</Anchors>
			</Button>
			<Button parentArray="pets" inherits="RematchTeamCardPetTemplate">
				<Anchors>
					<Anchor point="TOPLEFT" x="20" y="-102"/>
				</Anchors>
			</Button>
			<Button parentArray="pets" inherits="RematchTeamCardPetTemplate">
				<Anchors>
					<Anchor point="TOPLEFT" x="20" y="-152"/>
				</Anchors>
			</Button>

