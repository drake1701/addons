					<Anchors>
						<Anchor point="TOPLEFT" x="3" y="-37"/>
						<Anchor point="TOPRIGHT" x="-3" y="-37"/>
					</Anchors>
					<TexCoords left="0" right="1" top="0.0078125" bottom="0.078125"/>
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
