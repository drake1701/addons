	<!-- Dwarf Button -->
	<Button name="MATBDwarf" virtual="true" >
		<Size x="25" y="25"/>
		<Anchors>
			<Anchor point="TOPLEFT" x="15" y="-12"/>
		</Anchors>
		<Scripts>
			<OnClick>
				MinArch:DimHistoryButtons();
				self:SetAlpha(1.0);
				MinArchOptions['CurrentHistPage'] = 1;
				MinArch:CreateHistoryList(1);
				MinArch:CreateHistoryList(1);
			</OnClick>
			<OnEnter>
				MinArch:HistoryButtonTooltip(1)
			</OnEnter>
			<OnLeave>
				GameTooltip:Hide();
			</OnLeave>
		</Scripts>
		<NormalTexture file="Interface\Archeology\Arch-Race-Dwarf.blp">
			<Size x="30" y="30"/>	
			<TexCoords left="0.0234375" right="0.5625" top="0.078125" bottom="0.625"/>
		</NormalTexture>
		<PushedTexture file="Interface\Archeology\Arch-Race-Dwarf.blp">
			<Size x="30" y="30"/>	
			<TexCoords left="0.0234375" right="0.5625" top="0.078125" bottom="0.625"/>
		</PushedTexture>
		<HighlightTexture file="Interface\Archeology\Arch-Race-Dwarf.blp" alphaMode="ADD">
			<Size x="30" y="30"/>	
			<TexCoords left="0.0234375" right="0.5625" top="0.078125" bottom="0.625"/>
		</HighlightTexture>
	</Button>
	
